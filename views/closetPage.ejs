    <script type="text/javascript" src="/scripts/closetPage.js" ></script>
  </head>
  <body>
     <a href="/addClothing" class="btn btn-primary" id ="gotToAdd" type="submit">Add clothing</a> 
    <div class ="vertical-center">
      <div class="container vertical-center d-felx">
        <h2>Welcome to your personal closet</h2>
      </div>
    </div>
      <ul class="list">
        <% clothing.forEach(function(res){ %>
          <li> <%= res.typeOf %> <li/>
          <button>Delete</button>
          <button>Update</button>
        <% }); %>
      </ul>
  </body>
</html>